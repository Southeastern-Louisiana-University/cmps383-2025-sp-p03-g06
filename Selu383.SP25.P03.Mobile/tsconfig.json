{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [ "./*" ]
        }
    },
    "include": [
        "types",
        "app",
        "components",
        "hooks",
        "constants",
        "scripts",
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}
